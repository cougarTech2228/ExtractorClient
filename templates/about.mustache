{{> header}}
<div class="container">
    <div class="row">
        <div class="col s12">
            <h1>About</h1>
            <ul class="collection with-header">
                <li class="collection-item"><div><i class="material-icons">info</i> Version<div class="secondary-content">{{VERSION}}</div></div></li>
                <li class="collection-item"><div><i class="material-icons">perm_device_information</i> Device ID<div class="secondary-content">{{deviceID}}</div></div></li>
                <li class="collection-item"><div><i class="material-icons">input</i> Configured Team<div class="secondary-content">{{team}}</div></div></li>
                <li class="collection-item"><div><i class="material-icons">av_timer</i> Current Match<div class="secondary-content">{{currentMatch}}</div></div></li>
                <li class="collection-item"><div><i class="material-icons">av_timer</i> QR Swap Rate<div class="secondary-content">{{qrRateMS}}</div></div></li>
            </ul>

            <a class="waves-effect waves-light btn-large center"><i class="material-icons left">system_update_alt</i> Hot Swap Config</a>
        </div>
    </div>
</div>
{{> footer}}
