{{> header}}
{{> nav}}
<div class="container">
    <div class="row">
        <div class="col s12">
            <h1>Match List</h1>

            <ul class="collection with-header">
                <li class="collection-item">
                    <div>Match &num;
                        <div class="secondary-content" style="color: {{{teamColor}}};">{{team}}</div>
                    </div>
                </li>
                {{#matches}}
                    <li class="collection-item"><a href="{{BASEURI}}match/{{match}}">
                        <div>Match &num;{{match}}
                            <div class="secondary-content" style="color: {{{teamColor}}};">{{teamNum}}</div>
                        </div>
                    </a></li>
                {{/matches}}
            </ul>
        </div>
    </div>
</div>
{{> footer}}
